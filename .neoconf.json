{
  "neodev": {
    "library": {
      "enabled": true,
      "plugins": true
    }
  },
  "neoconf": {
    "plugins": {
      "lua_ls": {
        "enabled_for_neovim_config": true,
        "enabled": true
      }
    }
  },
  "lspconfig": {
    "lua_ls": {
      "Lua.diagnostics.disable": [
        "missing-fields",
        "unused-local"
      ],
      "Lua.format.enable": true
    }
  }
}
